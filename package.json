{
  "name": "fireblog-theme-seo-booster",
  "version": "1.0.0",
  "description": "eleventy disto blog for fireblog",
  "keywords": [],
  "license": "ISC",
  "author": "LÃ©o Gaboriau",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env ELEVENTY_ENV=development npm-run-all -p css:build css:watch eleventy:watch",
    "build": "cross-env ELEVENTY_ENV=production npm-run-all --sequential clean eleventy:build css:build",
    "clean": "rm -rf _site",
    "eleventy:build": "eleventy",
    "eleventy:watch": "eleventy --serve",
    "css:build": "postcss src/styles/main.css -o _site/assets/css/styles.css",
    "css:watch": "chokidar 'src/styles/**/*.css' --command 'npm run css:build'"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "autoprefixer": "^9.7.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "html-minifier": "^4.0.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^7.1.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.7.6"
  },
  "devDependencies": {
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
