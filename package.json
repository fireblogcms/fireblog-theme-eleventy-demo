{
  "name": "eleventy-starter-fireblog",
  "version": "1.0.0",
  "description": "eleventy starter blog for fireblog",
  "keywords": [],
  "license": "ISC",
  "author": "LÃ©o Gaboriau",
  "main": "index.js",
  "scripts": {
    "eleventy:build": "eleventy",
    "eleventy:watch": "eleventy --serve",
    "dev": "eleventy --serve",
    "css:build": "postcss src/styles/main.css -o src/static/assets/css/styles.css",
    "css:watch": "chokidar 'src/styles/**/*.css' --command 'npm run css:build'",
    "build": "cross-env ELEVENTY_ENV=production npm-run-all --sequential css:build eleventy:build",
    "watch": "cross-env ELEVENTY_ENV=development npm-run-all -p css:watch eleventy:watch"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "autoprefixer": "^9.7.5",
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "html-minifier": "^4.0.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^7.1.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.7.6"
  }
}
